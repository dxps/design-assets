	"editor.tokenColorCustomizations": {
		"[Monokai Pro (Filter Machine)]": {
			"keywords": "#bca7e6",
			"textMateRules": [
				{
					"scope": "keyword.other.rust",
					"settings": {
						"foreground": "#d0aa79"
					}
				},
				{
					"scope": "keyword",
					"settings": {
						"foreground": "#bca7e6"
					}
				}
			]
		},
		"[Arctis]": {
			"functions": "#75a3fa",
			"strings": "#aeb842",
			"types": "#75a3fa",
			"textMateRules": [
				{
					"scope": [
						"keyword",
						"keyword.operator",
						"keyword.other.rust",
						"keyword.other.fn.rust",
						"keyword.control",
						"storage.type.rust",
						"variable.language.self.rust",
						"source.rust"
					],
					"settings": {
						"foreground": "#76c9ca",
						"fontStyle": ""
					}
				},
				{
					"scope": [
						"entity.name.type",
						"entity.name.namespace.rust",
						"source.rust entity.name.type.trait",
						"support.constant.core.rust"
					],
					"settings": {
						"foreground": "#75a3fa"
					}
				},
				{
					"scope": [
						"meta.attribute.rust",
						"punctuation.definition.attribute.rust"
					],
					"settings": {
						"fontStyle": "italic",
						"foreground": "#959595"
					}
				},
				{
					"scope": [
						"entity.name.lifetime.rust",
						"storage.modifier.lifetime.rust"
					],
					"settings": {
						"foreground": "#ffff00"
					}
				},
				{
					"scope": ["support.type.marker.rust"],
					"settings": {
						"foreground": "#6766b3"
					}
				},
				{
					"scope": "string",
					"settings": {
						"foreground": "#90b6c0",
						"fontStyle": ""
					}
				},
				{
					"scope": ["keyword.other.unsafe.rust"],
					"settings": {
						"foreground": "#f44747"
					}
				},
				{
					"scope": ["storage.modifier.mut.rust"],
					"settings": {
						"fontStyle": ""
					}
				}
			]
		}
	},
